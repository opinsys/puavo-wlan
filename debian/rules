#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_install:
	$(MAKE) -C ap install prefix=/usr sysconfdir=/etc DESTDIR=$(CURDIR)/debian/puavo-wlanap
	$(MAKE) -C gw install prefix=/usr sysconfdir=/etc DESTDIR=$(CURDIR)/debian/puavo-wlangw
	$(MAKE) -C ap-dnsproxy install prefix=/usr sysconfdir=/etc DESTDIR=$(CURDIR)/debian/puavo-wlanap-dnsproxy
	$(MAKE) -C ap-mdns install prefix=/usr sysconfdir=/etc DESTDIR=$(CURDIR)/debian/puavo-wlanap-mdns

override_dh_install:
